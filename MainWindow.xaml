<Window x:Class="u1_03_JesseComputer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:u1_03_JesseComputer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="btnClickMe"
               Click="btnClickMe_Click"
                Content="Processor"/>
        <Image Source="http://cdn.pcpartpicker.com/static/forever/images/product/78d5a83458df76aaff6112c60d482ff2.256p.jpg"
               Visibility="Visible"
               x:Name="Processor"
               />


        <Button x:Name="btnClickMe2"
               Click="btnClickMe_Click"
                Content="Motherboard"/>
        <Image Source="http://www.tigerdirect.ca/applications/SearchTools/item-details.asp?EdpNo=10078229;
"
               Visibility="Visible"
               />

        <Button x:Name="btnClickMe3"
               Click="btnClickMe_Click"
                Content="CPU"/>
        <Image Source="https://www.newegg.com/Product/Product.aspx?Item=N82E16820231588"
               Visibility="Visible"
               />


    </StackPanel>
</Window>
